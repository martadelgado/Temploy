<ol class="breadcrumb">
<li><a href="#">Home</a></li>
<li><a href="/dashboard">Dashboard</a></li>
<li class="active">Edit Profile</li>
</ol>
<h1> Edit Your Profile</h1>
<div class ="col-md-6 col-md-offset-3">
  <form action="/dashboard/<%= user._id %>/edit" method="POST" enctype="multipart/form-data">
    <h3>Upload your photo</h3>
    <% pictures.forEach(function(picture) { %>
      <h3><%= picture.name %></h3>
      <img class="img-circle" src="<%= picture.pic_path %>" alt="<%= picture.pic_name %>" width="140" height="140" value="<%= picture._id%>"/>
      <a class = "link" href="/picture/<%=picture._id%>/delete">Delete </a>
      <% })%>
    <input type="text" name="namePic">
    <input type="file" name="file">
    <input type="submit" value="Save">
    </form>
    <form action="/dashboard/<%= user._id %>/editprofile" method="POST">
      <div class="form-group">
        <label for="nameInput">Name</label>
        <input type="text" class="form-control" id="name" name="name" value= <%= user.name%>>
      </div>
      <div class="form-group">
        <label for="lastNameInput">Last Name</label>
        <input type="text" class="form-control" id="lastName" name="lastName" value=<%= user.lastName%>>
      </div>
      <div class="form-group">
        <label for="dateOfBirth">Date of Birth</label>
        <input type="date" class="form-control" id="dateOfBirth" name="dateOfBirth" value=<%= user.dateOfBirth%>>
      </div>
      <button type="submit" class="btn-primary btn-block">Update Profile</button>
    </form>
  </div>
